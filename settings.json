{
  // "editor.formatOnSaveMode": "modifications",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.printWidth": 108,
  "prettier.trailingComma": "all",
  "editor.autoClosingOvertype": "always",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.snippetSuggestions": "top",
  "editor.lightbulb.enabled": false,
  "editor.wordBasedSuggestions": "currentDocument",
  "editor.detectIndentation": true,
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.autoIndent": "advanced",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordBasedSuggestions": "currentDocument",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.trimAutoWhitespace": false,
  "files.trimTrailingWhitespace": false,
  "editor.autoIndent": "advanced",
  "files.eol": "\n",
  "editor.links": false,
  "editor.tabCompletion": "on",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.parameterHints.enabled": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.formatOnPaste": false,
  "editor.wordWrap": "on",
  "editor.suggest.localityBonus": true,
  // "[csharp]": {
  //   "editor.suggest.localityBonus": false,
  //   "editor.maxTokenizationLineLength": 2500,
  //   "editor.defaultFormatter": "ms-dotnettools.csharp"
  // },
  "editor.renderWhitespace": "boundary",
  "editor.autoClosingDelete": "always",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "files.associations": {
    "*.env": "env",
    "*.env.development": "env",
    "*.env.production": "env",
    "package.json": "json",
    "*.json": "jsonc",
    "*.yaml": "yaml"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.unusedImports": "explicit",
    "source.fixAll": "explicit"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8",
    "editor.tabSize": 4,
    "editor.autoClosingOvertype": "always",
    "editor.wordBasedSuggestions": "currentDocument",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.unusedImports": "explicit"
    }
  },
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.autoImportCompletions": true,
  "autopep8.args": [
    "--max-line-length",
    "108",
    // "--indent-size",
    // "2",
    "--ignore",
    "E121"
    // "--ignore",
    // "E302"
  ],
  // C:\Users\pink\AppData\Roaming\Code\User\settings.json
  "files.autoGuessEncoding": true,
  "search.exclude": {
    "LICENSE": true,
    // "**/settings.json": true,
    // ".vscode/settings.json": true,
    // ".vscode/settings.json": true,
    ".gitignore": true,
    ".vscodeignore": true,
    "**/*.md": true,
    "**/*.map": true,
    "**/*.d.ts": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/css": true,
    "**/*.min.js": true,
    "**/*.css": true,
    ".github": true
  },
  "files.exclude": {
    "package-lock.json": true,
    "yarn.lock": true,
    "Cargo.lock": true,
    "pnpm-lock.yaml": true,
    ".venv": true,
    ".next": true,
    "bin": true,
    ".vscode-test": true,
    "*/obj/Debug": true,
    // "**/migrations": true,
    "**/node_modules": true,
    "**/__snapshots__": true,
    "**/__pycache__": true,
    // "**/*.exe": true,
    // "**/*.class": true,
    "**/*.tsv": true
  },
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorTheme": "Monokai",
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "explorer.copyRelativePathSeparator": "/",
  "explorer.confirmDragAndDrop": false,
  // "debug.console.historySuggestions":false,
  "debug.onTaskErrors": "debugAnyway",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enableFileLinks": "off",
  "security.workspace.trust.untrustedFiles": "open",
  "emmet.syntaxProfiles": {
    "html": {
      "inline_break": 2
    }
  },
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "none",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "[markdown]": {
    // "editor.autoClosingDelete": "always",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "problems.autoReveal": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "search.followSymlinks": false,
  // "window.title": "${dirty}${activeEditorLong}${separator}${rootName}",
  "window.title": "${rootPath}",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "bookmarks.navigateThroughAllFiles": false,
  "screencastMode.fontSize": 20,
  // "npm.packageManager": "pnpm",
  // "http.proxy": "http://127.0.0.1:10809",
  "http.proxy": "http://172.27.192.1:10811",
  "[json]": {
    "editor.tabSize": 4
    // "matchDepNames": ["typescript"],
    // 删除相邻的右引号或右括号(即成对删除) 这个设置是需要的
    // "editor.autoClosingDelete": "never"
  },
  "debug.toolBarLocation": "hidden",
  // @tailwind base;
  "css.lint.unknownAtRules": "ignore", // Unknown at rule @tailwind
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "editor.accessibilitySupport": "off",
  "workbench.editor.enablePreview": false,
  "security.allowedUNCHosts": ["wsl.localhost"],
  "editor.codeLens": false,
  "editor.acceptSuggestionOnCommitCharacter": false, // 鸡肋
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "explorer.confirmDelete": false,
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.activityBar.location": "hidden",
  // "[sql]": {
  //   "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
  // },
  "SQL-Formatter-VSCode.dialect": "postgresql",
  "SQL-Formatter-VSCode.indentStyle": "tabularLeft",
  "SQL-Formatter-VSCode.paramTypes": {
    "custom": [
      {
        "regex": "\\$\\{.*\\}"
      }
    ]
  },
  "SQL-Formatter-VSCode.expressionWidth": 80,
  "SQL-Formatter-VSCode.keywordCase": "upper",
  "SQL-Formatter-VSCode.dataTypeCase": "upper",
  "SQL-Formatter-VSCode.functionCase": "upper",
  "workbench.editor.dragToOpenWindow": false,
  "explorer.confirmPasteNative": false,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker",
    "editor.tabSize": 4
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  // "[rust]": {
  //   "editor.autoClosingOvertype": "always",
  //   "editor.defaultFormatter": "rust-lang.rust-analyzer",
  //   "editor.tabSize": 2,
  //   "editor.codeActionsOnSave": {
  //     "source.organizeImports": "always",
  //     "source.fixAll": "always"
  //   }
  // },
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.completion.callable.snippets": "none",
  "rust-analyzer.completion.postfix.enable": false,
  "debug.allowBreakpointsEverywhere": true,
  // "editor.guides.indentation": false 缩进参考线, 不要关
  // aa==aa"Hello".toCharArray() 有得必有舍
  // "debug.console.acceptSuggestionOnEnter": "on",
  "redhat.telemetry.enabled": true,
  // "[java]": {
  //   "editor.autoClosingOvertype": "always",
  //   // "editor.autoClosingDelete": "never",
  //   "editor.defaultFormatter": "redhat.java"
  // },
  "java.debug.settings.onBuildFailureProceed": true,
  "java.inlayHints.parameterNames.enabled": "none",
  "java.completion.guessMethodArguments": "off",
  "java.completion.postfix.enabled": false,
  "java.completion.chain.enabled": true,
  "java.completion.matchCase": "off",
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  // "git.confirmSync": false,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  // "[csharp]": {
  //   "editor.defaultFormatter": "ms-dotnettools.csharp"
  // },
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    }
  },
  "gopls": {
    "ui.semanticTokens": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }

  // "[jsonc]": {
  //   // prettier会删掉最后一行的`,`
  //   // 各有各的好 这个会删掉所有空行
  //   "editor.defaultFormatter": "vscode.json-language-features"
  // }
  // ,"editor.foldingImportsByDefault": false,
}
